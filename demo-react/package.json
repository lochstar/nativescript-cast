{
    "name": "@nativescript-community/template-react",
    "version": "0.0.1",
    "main": "app.js",
    "homepage": "https://github.com/nativescript-community/plugin-seed",
    "bugs": {
        "url": "https://github.com/nativescript-community/plugin-seed/issues"
    },
    "scripts": {
        "postinstall": "patch-package",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "clean": "npx rimraf node_modules package-lock.json platforms hooks",
        "format": "npx prettier --write './**/*.{ts,js,jsx,tsx,md,html,css,scss,json}'",
        "lint": "npm run format && npx eslint . --ext .ts,.js,.jsx,tsx, --fix"
    },
    "dependencies": {
        "@nativescript/core": "~7.0.13",
        "@nativescript/detox": "^1.0.0",
        "@nativescript/theme": "^3.0.0",
        "@nativescript-community/plugin-seed": "../plugin",
        "@react-navigation/native": "^5.4.3",
        "react": "^16.13.1",
        "react-nativescript": "^2.1.0",
        "react-nativescript-navigation": "^2.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@nativescript/android": "7.0.1",
        "@nativescript/ios": "7.0.4",
        "@nativescript/types": "^7.0.4",
        "@nativescript/webpack": "~3.0.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.0-beta.5",
        "@types/react": "16.9.34",
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "@typescript-eslint/parser": "^4.7.0",
        "babel-loader": "8.0.6",
        "detox": "^17.14.1",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "fork-ts-checker-webpack-plugin": "^5.1.0",
        "patch-package": "^6.2.2",
        "prettier": "^2.1.2",
        "react-refresh": "^0.8.3",
        "sass": "^1.29.0",
        "typescript": "~4.0.4"
    }
}

{
    "name": "@codelab/nativescript-cast",
    "version": "1.0.0",
    "description": "NativeScript Chromecast Plugin.",
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "Chromecast",
        "Cast"
    ],
    "homepage": "https://github.com/lochstar/nativescript-cast#readme",
    "bugs": {
        "url": "https://github.com/lochstar/nativescript-cast/issues"
    },
    "license": "ISC",
    "author": {
        "name": "Lochlan McBride",
        "email": "lochlan@gmail.com"
    },
    "scripts": {
        "build": "lerna run build",
        "build.all": "lerna run build.all",
        "build.angular": "lerna run build.angular",
        "clean": "rimraf packages/**/*.d.ts packages/**/*.js packages/**/*.js.map packages/**/*.metadata.json packages/**/*.ngsummary.json node_modules package-lock.json",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "demo.ng.android": "cd ./demo-ng && ns run android --no-hmr",
        "demo.ng.ios": "cd ./demo-ng && ns run ios --no-hmr",
        "demo.react.android": "cd ./demo-react && ns run android --no-hmr",
        "demo.react.ios": "cd ./demo-react && ns run ios --no-hmr",
        "demo.svelte.android": "cd ./demo-svelte && ns run android --no-hmr",
        "demo.svelte.ios": "cd ./demo-svelte && ns run ios --no-hmr",
        "demo.vue.android": "cd ./demo-vue && ns run android --no-hmr",
        "demo.vue.ios": "cd ./demo-vue && ns run ios --no-hmr",
        "postinstall": "npm run setup",
        "publish": "npm run setup && npm run build.all && lerna publish --create-release=github --force-publish",
        "readme": "node ./tools/readme.js",
        "setup": "npm run submodules && ts-patch install",
        "start": "./node_modules/.bin/ntl -A -s 15 -o",
        "submodules": "git submodule update --init",
        "sync": "node ./tools/sync.js -w && npx sort-package-json",
        "sync.test": "node ./tools/sync.js",
        "tsc": "cpy '**/*.d.ts' '../plugin' --parents --cwd=src && tsc -skipLibCheck -d",
        "update.seed": "git submodule update --recursive --remote",
        "watch": "npm run tsc -- -w"
    },
    "devDependencies": {
        "@angular/animations": "~12.2.9",
        "@angular/common": "~12.2.9",
        "@angular/compiler": "~12.2.9",
        "@angular/compiler-cli": "~12.2.9",
        "@angular/core": "~12.2.9",
        "@angular/forms": "~12.2.9",
        "@angular/platform-browser": "~12.2.9",
        "@angular/platform-browser-dynamic": "~12.2.9",
        "@angular/router": "~12.2.9",
        "@appnest/readme": "^1.2.7",
        "@commitlint/cli": "~13.2.1",
        "@commitlint/config-conventional": "~13.2.0",
        "@nativescript/angular": "~12.2.0",
        "@nativescript/core": "~8.1.4",
        "@nativescript/eslint-plugin": "0.0.4",
        "@nativescript/ios": "~8.1.0",
        "@nativescript/types-android": "~8.1.0",
        "@nativescript/types-ios": "~8.1.1",
        "@nativescript/webpack": "~5.0.0",
        "@types/node": "14.6.4",
        "@types/react": "16.9.34",
        "@typescript-eslint/eslint-plugin": "5.0.0",
        "@typescript-eslint/parser": "5.0.0",
        "cpy-cli": "^3.1.1",
        "eslint": "8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-svelte3": "3.2.1",
        "eslint-plugin-vue": "^7.19.1",
        "husky": "^7.0.2",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-cli": "^26.6.3",
        "json-merge": "^1.2.0",
        "lerna": "^4.0.0",
        "lint-staged": "^10.5.3",
        "minimist": "^1.2.5",
        "nativescript-vue": "~2.9.0",
        "ng-packagr": "~12.2.3",
        "npm-deps": "^0.3.7",
        "ntl": "^5.1.0",
        "package-json-merge": "0.0.1",
        "patch-package": "~6.4.7",
        "prettier": "^2.4.1",
        "prompt": "~1.2.0",
        "react-nativescript": "3.0.0-beta.3",
        "rimraf": "~3.0.2",
        "rxjs": "~6.5.5",
        "svelte": "3.43.1",
        "svelte-native": "~1.0.0",
        "ts-node": "~9.1.1",
        "ts-patch": "~1.4.4",
        "tslib": "2.3.1",
        "typedoc": "~0.22.5",
        "typescript": "~4.2.3",
        "vue": "~2.6.14",
        "yargs": "^17.2.1",
        "zone.js": "~0.11.4"
    },
    "nativescript": {
        "platforms": {
            "android": "8.0.0",
            "ios": "8.0.0"
        }
    },
    "ntl": {
        "descriptions": {
            "build": "Build the plugin",
            "build.angular": "Build the plugin for Angular",
            "build.all": "Build the plugin for all platforms",
            "clean": "Clean the local environment.",
            "demo.ng.android": "Runs the Angular demo on Android.",
            "demo.ng.ios": "Runs the Angular demo on iOS.",
            "demo.react.android": "Runs the React demo on Android.",
            "demo.react.ios": "Runs the React demo on iOS.",
            "demo.svelte.android": "Runs the Svelte demo on Android.",
            "demo.svelte.ios": "Runs the Svelte demo on iOS.",
            "demo.vue.android": "Runs the Vue demo on Android.",
            "demo.vue.ios": "Runs the Vue demo on iOS.",
            "watch": "Watch for changes in the plugin source and re-build."
        }
    }
}
